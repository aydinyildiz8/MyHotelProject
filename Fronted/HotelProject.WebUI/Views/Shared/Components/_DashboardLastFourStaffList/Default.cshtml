@model List<ResultLastFourStaffDto>


<div class="row">

    @foreach (var item in Model)
    {
        <div class="col-lg-3 col-sm-6">
            <div class="card">
                <div class="card-body">
                    <div class="text-center">
                        <img src="@item.StaffImaage" class="rounded-circle img-fluid" alt="">
                        <h5 class="mt-3 mb-1">@item.StaffName</h5>
                        <p class="m-0">@item.StaffTitle</p>
                        <!-- <a href="javascript:void()" class="btn btn-sm btn-warning">Send Message</a> -->
                    </div>
                </div>
            </div>
        </div>
    }

</div>
@model List<ResultAppUserDto>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
    int count = 0;
}


<div class="active-member">
    <div class="table-responsive">
        <table class="table table-xs mb-0">
            <thead>
                <tr>
                    <th>#</th>
                    <th>Kullanıcı Adı</th>
                    <th>Adı Soyadı</th>
                    <th>Mail Adresi</th>
                    <th>Şehir</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    count++;

                    <tr>
                        <td>@count</td>
                        <td><img src="@item.ImageUrl" class=" rounded-circle mr-3" alt="">@item.UserName</td>
                        <td>@item.Name @item.Surname</td>
                        <td>@item.Email</td>
                        <td>
                            <span>@item.City</span>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
        <a href="/Register/Index/" class="btn btn-primary mt-4">Yeni Kullanıcı Kaydı</a>
    </div>
</div>

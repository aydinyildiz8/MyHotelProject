@model List<BookingApiViewModel.Result>

@{
    Layout = null;
    int count = 0;
}

<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />

<div class="container">
    <br />
    <br />
    <table class="table table-bordered">

        <tr>
            <th>#</th>
            <th>Otel Adı</th>
            <th>Puanı</th>
            <th>Detayı</th>
        </tr>

        @foreach (var item in Model)
        {
            count++;
            <tr>
                <th>@count .</th>
                <td>@item.name</td>
                <td>@item.reviewScore</td>
                <td><a href="#" class="btn btn-dark">Detay</a></td>
            </tr>
        }
    </table>
</div>